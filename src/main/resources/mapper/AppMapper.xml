<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--namespace:映射命名空间-->
<mapper namespace="cn.hsf.hsf.mapper.app.AppMapper">

    <!--  App selapp(); -->
    <select id="selapp" resultType="cn.hsf.hsf.pojo.app.App">
        SELECT `id`, `appId`, `mchId`, `appSecret`, `token`, `key`
        FROM `tb_app`
    </select>



</mapper>